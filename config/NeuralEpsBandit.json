{
    "name": "NeuralEps-Default",
    "type": "Bandit",
    "allocator":{
        "type": "NeuralAllocator",
        "kwargs":{
            "lr": 1e-3,
            "batch_size": 256,
            "weight_decay": 1e-6,
            "latent_dim": 32,
            "num_epochs": 10,
            "mode": "\"Epsilon-greedy\"",
            "eps_min": 0.01,
            "eps_max": 1.0
        }
    },
    "bidder":{
        "type": "DefaultBidder",
        "kwargs":{
            "lr": 1e-3,
            "batch_size": 256,
            "num_grad_steps": 10
        }
    },
    "exploration_length": 0,
    "output_dir": "results/NeuralEps_Default"
}